$header-heigth: 130px; 
$bree: bree, Helvetica, Arial, sans-serif;
$frutiger: myriad-pro, "DejaVu Sans Condensed", "Liberation Sans", "Nimbus Sans L", Tahoma, "Helvetica Neue", Helvetica, Arial, sans-serif;

body {
  background: #d3d3d3 url('Philadelphia-site-home-BG.jpg');
  overflow-y: scroll;
  font-family: $frutiger;
}
.nav-silver{
  width:56px!important;
}
.hidden {
  display: none;
}
.lazy{
  max-width: 10000px;
  height: 0;
  display: none;
}
img.video-embed{
  cursor: pointer;
  cursor: hand;
}
span.video-embed, div.video-embed{
  display: block;
  padding: 0 !important;
  object{
    width: 100%;
    height: 100%;
  }
}
.inlineblock, .inline-block{
  display: -moz-inline-stack;
  display: inline-block;
  vertical-align: top;
  zoom: 1;
  *display: inline;
}
.flechas{
  display:none;
  z-index: 2;
  position: fixed;
  bottom: 0px;
  left:0;
  width: 100%;
  height: 50px;
  background: image-url('recetas/flecha.png') no-repeat center 0;
  .scrolls{
    margin:0 auto;
    width:200px;
    height:30px;
    margin-top:10px;
    cursor:pointer;
  }
}
.bg-icon{
  span{
    display: none;
  }
}
.ctr{ text-align: center; }
.rgt{ text-align: right; }
.lft{ text-align: left; }

#reveal-video-global{
  width:640px;
  .close-reveal-modal{
    background: image-url('videos/close.png') no-repeat 0 0;
    display:block;
    width:45px;
    height:44px;
    text-indent:-9999px;
  }   
  h1{
    font-family:$bree;
    font-weight: 400;
    font-size: 25px;
    color: #29ABE1;
  }
  .widescreen{
    height:1px!important;
  }
}

.fb-like{ 
  height:30px;
}
.container {
  > header {
    $header-width: 1035px;
    
    position: fixed;
    width: $header-width;
    left: 50%;
    margin-left: (-1*$header-width/2);
    background: url(Philadelphia-site-home-BG.jpg) top center; 

    z-index: 10;
  

    > nav {
      background: #26b0f0;
      position: relative;
      z-index: 100;
      top: 20px;
      height: 38px;
      margin-top: 14px;
      padding: 0 25px;
      border-radius: 10px;
      font-size: 13px;

      a {
        display: block;
        float: left;
        width: 75px;
        min-height: 38px;
        text-align: center;
        color: white;
        font-weight: bold;
        text-shadow: 0 0 4px #666;
        line-height: 38px;
        &.twolines {
          padding-top: 3px;
          line-height: 16px;
        }
        &:hover, &.active {
          background: image-url('layout/degradado1mas.png') 0 0 repeat-x;
        }
        &.foodservice {
          word-spacing: 10px;
        }
        &.preguntaleayaz{
          padding-left: 2px;
          padding-right: 2px;
        }
        &.nosotrasyphiladelphia{
          padding-right: 10px;
          padding-left: 10px;
        }
        &.basicos{
          padding-left: 3px;
          padding-right: 3px;
          margin-right: -6px;
        }
        &.recompensas{
          position: absolute;
          top: -1px;
          right: 0;
          width: 149px;
          background: image-url('promos/menu-recompensas.png') 0 0 no-repeat;
        }
      }

      > h1 > a {
        margin-top: 0;
        width: 198px;
        height: 100px;
        text-indent: -99999px;
        background: image-url('layout/logo.png') 0 0 no-repeat;

        &:hover, &.active {
          background: image-url('layout/logo.png') 0 0 no-repeat;
        }
      }
    }

    menu.secundario, menu.ayuda, menu.extra{
      ul{
        text-align: right;
        li{
          display: inline;
          a.inlineblock{
            width: 26px;
            height: 26px;
            background-repeat: no-repeat;
            background-position: center center;
            padding: 5px;
            span{
              display: none;
            }
          }
          a.twitter{
            background-image: image-url('layout/icono_twitter.png');
          }
          a.facebook{
            background-image: image-url('layout/icono_fb.png');
          }
          a.youtube{
            background-image: image-url('layout/icono_youtube.png');
          }
          a.blogger{
            background-image: image-url('layout/icono_blogger.png');
          }
          a.gplus{
            background-image: image-url('layout/icono_gplus.png');
          }
        }
      }
    }
    menu.secundario{
      width: 300px;
      height: 26px;
      position: absolute;
      top: 10px;
      right: 15px;
      white-space:nowrap;
      ul{
        text-align: right;
        li{
          font-family: $bree;
          font-weight: 200;
          color: #294B94;
          display: inline;
          a.inlineblock{
            width: 26px;
            height: 26px;
            background-repeat: no-repeat;
            background-position: center center;
            padding: 5px;
            span{
              display: none;
            }
          }
          a.twitter{
            background-image: image-url('layout/icono_twitter.png');
          }
          a.facebook{
            background-image: image-url('layout/icono_fb.png');
          }
          a.youtube{
            background-image: image-url('layout/icono_youtube.png');
          }
          a.blogger{
            background-image: image-url('layout/icono_blogger.png');
          }
          a.gplus{
            background-image: image-url('layout/icono_gplus.png');
          }
        }
      }
    }
    menu.ayuda{
      display: none;
      position: absolute;
      top: 70px;
      right: -1px;
      width: 103px;
      height: 135px;
      background: #29ABE2;
      overflow: hidden;
      border-radius: 0 0 10px 10px;
      ul{
        text-align: center;
        padding: 0;
        margin: 30px 0 0;
        li{
          padding: 0;
          margin: 0;
          a.inlineblock{
            width: 34px;
            height: 38px;
          }
          a.twitter{
            background-image: image-url('layout/ayuda_twitter.png');
          }
          a.facebook{
            background-image: image-url('layout/ayuda_facebook.png');
          }
          a.callcenter{
            background-image: image-url('layout/ayuda_callcenter.png');
          }
          a.email{
            background-image: image-url('layout/ayuda_email.png');
          }
        }
      }
    }
    menu.extra{
      position: absolute;
      top: 10px;
      left: 20px;
      > ul{
        margin: 0;
        > li{
          a.bg-icon{
            background: none no-repeat 0 0;
            width: 20px;
            height: 10px;
            cursor: pointer;
          }
          &.recompensas{
            a{
              width: 70px;
              height: 36px;
              background-image: image-url('layout/icono_recompensas.png');
            }
          }
          &.chat {
            a {
              width: 37px;
              height: 37px;
              background-image: image-url('layout/icono_chat.png');
            }
          }
          &.boletin{
            a{
              width: 140px;
              height: 44px;
              background-image: image-url('layout/icono_boletin.png');
              margin-top:-6px;
            }
          }
        }
      }
    }
  }
  > #main {
    
    width: 960px;
    height: auto;
    margin: 0 auto;
    overflow-x: hidden;
    overflow-y: hidden;
    
    position: relative;
  }
}

#w_preindex{
  width: 1600px; 
  height: 900px; 
  float: left; 
  background:#FFF; 
  background: url("https://s3.amazonaws.com/philly.2013/assets/home-redesing/preindex.jpg") no-repeat; padding-left: 20px;
}

section.panel {
  width: 960px;
  #min-height: 7757px;
  float: left;
  background-attachment: fixed;
  background-position: 50% 0;
  background-repeat: no-repeat;
  position: relative;
  article {
    padding-top: 243px;
    padding-left: 130px;
    min-height: 500px;
    max-width: 960px;
    &.ghost{
      min-height: 180px;
    }
    > header {
      font-weight: 400;
      font-size: 2em;
      text-align: center;
      text-shadow: 1px -1px 4px rgba(0,0,0,0.4);
      width: 700px;
      
      font-family: bree, Helvetica, Arial, sans-serif;
    }
    .main-section {
      margin-top: 45px;
      padding-right: 110px;

      img.main-image {
        display: block;
        margin: 0 auto;
      }

      > aside {
        padding: 10px;
        width: auto;
        max-width: 228px;
        border-radius: 10px;
        > h4, p, li {
          font-size: 1em;
        }
        ul {
          list-style: disc inside;
          > li {
            margin: 0;
          }
        }
      }
    }
    .slogan {
      p {
        font-family: $bree;
        font-weight:  200;
      }
    }
  }
  > nav {
  
    position: absolute;
    //background: white;
    background-image: linear-gradient(top, rgba(0,42,81,.35) 16%, rgba(0,153,204,.35) 52%, rgba(150,216,247,.35) 90%);
    background-image: -o-linear-gradient(top, rgba(0,42,81,.35) 16%, rgba(0,153,204,.35) 52%, rgba(150,216,247,.35) 90%);
    background-image: -moz-linear-gradient(top, rgba(0,42,81,.35) 16%, rgba(0,153,204,.35) 52%, rgba(150,216,247,.35) 90%);
    background-image: -webkit-linear-gradient(top, rgba(0,42,81,.35) 16%, rgba(0,153,204,.35) 52%, rgba(150,216,247,.35) 90%);
    background-image: -ms-linear-gradient(top, rgba(0,42,81,.35) 16%, rgba(0,153,204,.35) 52%, rgba(150,216,247,.35) 90%);
		
		/*agregado redesign*/
		margin-top:80px;
		
    left: 20px;
    top: 120px;
    width: 40px;
    margin-left: 20px;

    -webkit-transition: all 0.4s ease-in-out;
    -moz-transition: all 0.4s ease-in-out;
    -ms-transition: all 0.4s ease-in-out;
    -o-transition: all 0.4s ease-in-out;
    transition: all 0.4s ease-in-out;

    &.silver {
      background-image:         linear-gradient(top, rgba(70,80,83,.35) 0%, rgba(139,146,152,.35) 48%, rgba(224,224,224,.35) 100%);
      background-image:      -o-linear-gradient(top, rgba(70,80,83,.35) 0%, rgba(139,146,152,.35) 48%, rgba(224,224,224,.35) 100%);
      background-image:    -moz-linear-gradient(top, rgba(70,80,83,.35) 0%, rgba(139,146,152,.35) 48%, rgba(224,224,224,.35) 100%);
      background-image: -webkit-linear-gradient(top, rgba(70,80,83,.35) 0%, rgba(139,146,152,.35) 48%, rgba(224,224,224,.35) 100%);
      background-image:     -ms-linear-gradient(top, rgba(70,80,83,.35) 0%, rgba(139,146,152,.35) 48%, rgba(224,224,224,.35) 100%);
    }

    > a {
      display: block;
      width: 80px;
      margin: 10px 0 10px -29px;
      > img {
        display: block;
        margin: 0 auto;
      }
      &.active {
        position: relative;
      }
      .flecha{
        position: absolute;
        top: 50%;
        right: -5px;
        margin: -11px 0 0 0 !important;
      }
    }
    .flecha{
      display: none;
    }
    a > .flecha{
      margin: 18px 0 0 93px;
      width: 12px;
      height: 22px;
      /*background: image-url('layout/flecha-der.png');*/
    }
    a.active > .flecha{
      display: block;
    }
  }
  footer.pie{
    font: $frutiger;
    position: relative;
    z-index: 3;
    color: #214E9F;
    background: image-url('layout/footer.png');
    width: 960px;
    height: 88px;
    h3{
      position: absolute;
      top: 20px;
      left: 20px;
      text-indent: -9999px;
      width: 140px;
      height: 45px;
      display: block;
    }
    .mas{
      position: absolute;
      right: 50px;
      top: 25px;
      h5{
        color: #214E9F;
        font-size: 11px;
      }
      select{
        font-size: 10px;
      }
    }
    ul.modal-links{
      font-size: 11px;
      position: absolute;
      top: 35px;
      left: 210px;
      li{
        display: inline;
      }
      li + li:before{
        content: '|';
        color: rgba(33,78,159,0.5);
        padding: 0 0.4em;
      }
    }
    a{
      color: #214E9F;
    }
  }
}

